{% block navigation %}
  <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
    <span class="fa fa-bars">Toggle navigation</span>
  </button>
  <div class="branding-bottom clearfix">
    <nav class="main-navigation" role="navigation">
      <div class="menu-main-navigation-container">
        <ul class="ahip-menu menu">
        {% for nav in prototype_nav %}
          <li class="menu-item"><a href="{{ nav.link }}">{{ nav.name }}</a></li>
        {% endfor %}
        </ul>
      </div>
    </nav>
  </div>
{% endblock %}
